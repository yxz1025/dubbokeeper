*
*/
{
  "mappings": {
    "xxx":{
      "properties": {
        "timestamp": {
          "type":"date",
          "format":"strict_date_optional_time||epoch_millis"
        },
        "serviceInterface": {
          "type": "keyword"
        },
        "method": {
          "type": "keyword"
        },
        "type": {
          "type": "keyword"
        },
        "tps":{
          "type": "float"
        },
        "kbps": {
          "type": "float"
        },
        "host": {
          "type": "keyword"
        },
        "elapsed": {
          "type": "integer"
        },
        "concurrent": {
          "type": "integer"
        },
        "input": {
          "type": "integer"
        },
        "output": {
          "type": "integer"
        },
        "successCount": {
          "type": "integer"
        },
        "failureCount": {
          "type": "integer"
        },
        "remoteAddress": {
          "type": "keyword"
        },
        "remoteType": {
          "type": "keyword"
        }
      }
    }
  }
